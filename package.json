    "url": "https://github.com/RaiconY/Kate-Lash-Pitch/issues"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "eslint": "^8.57.0",
    "html-validate": "^8.7.4",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "stylelint": "^15.11.0",
    "uglify-js": "^3.17.4"
  },
  "engines": {
